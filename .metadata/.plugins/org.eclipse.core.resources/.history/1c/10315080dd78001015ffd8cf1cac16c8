package variableTest;

public class VariableTest {
	public static void main(String[] args) {
//	초기값
//	정수(int) : 0
//	실수(double) : 0,0
//	문자(char) : ' ' (공백문자)
//	문자열(String) : null
	
		int num1 = 10;
		double num2 = 10.7;
		float num3 = 10.7F;
		String hello = "안녕";
		char letter = "가";
		
		
	}
}
