package variableTest;

public class VariableTask01 {
	public static void main(String[] args) {
		int num1 = 0;
		boolean trueOrFalse = false;
		double num2 = 0;
		char letter = ' ';
		float num3 = 0.0F;
		String language = " ";
		Long num4 = 0L;
		
		int num1 =5;
		boolean trueOrFalse = true;
		double num2 = 7845.15;
		char letter = 'B';
		float num3 = 85.10F;
		String language = "자바";
		
	}
}
